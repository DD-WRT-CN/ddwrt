s,@AUTOCONF@,${SHELL} /home/dd-wrt/DEV/src/router/glib/missing --run autoconf,;t t
s,@AUTOMAKE@,${SHELL} /home/dd-wrt/DEV/src/router/glib/missing --run automake-1.7,;t t
s,@AUTOHEADER@,${SHELL} /home/dd-wrt/DEV/src/router/glib/missing --run autoheader,;t t
