s,@host_cpu@,mipsel,;t t
s,@host_vendor@,unknown,;t t
s,@host_os@,linux-gnu,;t t
s,@CC@,mipsel-linux-uclibc-gcc,;t t
s,@CFLAGS@,-pipe -Os -fomit-frame-pointer -mips32 -mtune=mips32 -funit-at-a-time -Wall  -D_REENTRANT,;t t
s,@LDFLAGS@,-s,;t t
