#!/bin/sh
LC_ALL=C
export LC_ALL
echo
echo Generating Unicode upcase table.
(
echo '/* Automatically generated by gen-upc */'
echo
echo 'struct { int lo; int up; } unicode_upcase [] = {'
cat upcase.uni | sed 's/^[ 	]*//' | sed 's/[ 	]*$//' | sed 's/  */, /' | sort -n | sed 's/^/	{ /' | sed 's/$/ },/'
echo '};'
) > ../upcase.inc
echo Done.
echo
