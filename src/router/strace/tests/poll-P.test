. "${srcdir=.}/init.sh"

run_prog > /dev/null
run_strace -a16 -vepoll -P /dev/full 9>>/dev/full \
	$args > "$EXP"
match_diff "$LOG" "$EXP"

for abbrev in 0 1 2 3 4 5; do
	run_prog "../$NAME" $abbrev > /dev/null
	run_strace -a16 -epoll -s$abbrev -P /dev/full 9>>/dev/full \
		$args > "$EXP"
	match_diff "$LOG" "$EXP"
done
