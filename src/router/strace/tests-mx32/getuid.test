. "${srcdir=.}/init.sh"

check_prog uniq
if [ "${NAME%-creds}" = "$NAME" ]; then
	syscall="$NAME"
else
	NAME="${NAME%-creds}"
	syscall='%creds'
fi
run_prog > /dev/null
run_strace -qq -a9 -e"$syscall" $args > "$EXP"
uniq < "$LOG" > "$OUT"
match_diff "$OUT" "$EXP"
