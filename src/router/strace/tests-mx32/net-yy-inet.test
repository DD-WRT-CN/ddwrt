. "${srcdir=.}/init.sh"

check_prog sed

run_prog ../netlink_inet_diag
run_prog > /dev/null
run_strace -a22 -yy -eclose,network $args > "$EXP"

# Filter out close() calls made by ld.so and libc.
sed -n '/socket/,$p' < "$LOG" > "$OUT"
match_diff "$OUT" "$EXP"
