                       { "name"          : "AvailableDataClasses",
                         "format"        : "guint32",
                         "public-format" : "MbimDataClass" },
                       { "name"          : "CurrentCellularClass",
                         "format"        : "guint32",
                         "public-format" : "MbimCellularClass" },
                       { "name"   : "ProviderId",
                         "format" : "string" },
                       { "name"   : "ProviderName",
