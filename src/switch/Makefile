all:
	gmake -C swlib  all;
	gmake -C swmod  all;
	gmake -C swdiag  all;
clean:
	gmake -C swlib  clean;
	gmake -C swmod  clean;
	gmake -C swdiag  clean;
rebuild:
	gmake -s -C swlib  rebuild;
	gmake -s -C swmod  rebuild;
	gmake -s -C swdiag  rebuild;
install:
	gmake -C swlib  install;
	gmake -C swmod  install;
	gmake -C swdiag  install;
