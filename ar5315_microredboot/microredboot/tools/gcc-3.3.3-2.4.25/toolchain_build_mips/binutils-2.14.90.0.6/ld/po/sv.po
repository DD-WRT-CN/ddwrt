msgid "  --kill-at                          Remove @nn from exported symbols\n"
msgstr "  --kill-at                          Ta bort @nn från exporterade symboler\n"

#: emultempl/pe.em:341
msgid "  --out-implib <file>                Generate import library\n"
msgstr "  --out-implib <fil>                 Generera importbibliotek\n"

